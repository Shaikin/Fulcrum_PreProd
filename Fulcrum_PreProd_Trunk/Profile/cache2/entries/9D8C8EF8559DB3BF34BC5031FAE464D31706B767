  data-versions="/EN-us/firefox/addon/11900/versions/"
  data-name="FirePath"
  
  data-min="4.0"
  data-max="43.*"
  data-is-compatible="true"
  data-is-compatible-app="true"
  data-compat-overrides="[]"
>
